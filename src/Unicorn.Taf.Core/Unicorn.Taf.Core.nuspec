<?xml version="1.0"?>
<package >
  <metadata>
    <id>Unicorn.Taf.Core</id>
    <version>3.0.0</version>
    <title>Unicorn.Taf.Core</title>
    <authors>Vitaliy Dobriyan</authors>
    <license type="expression">MIT</license>
    <copyright>Copyright © 2022 Vitaliy Dobriyan</copyright>
    <projectUrl>https://bitbucket.org/dobriyanchik/unicorntaf</projectUrl>
    <repository type="git" url="https://bitbucket.org/dobriyanchik/unicorntaf"/>
    <icon>images\icon.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Unicorn is test automation framework which provides wide spectrum of functionality out of box to start efficient test automation process in short terms.
      Unicorn features: asserts with collection of matchers, functionality for test steps implementation, parameterized tests and suites, tests dependency.
      This package includes core of unicorn automation framework, which is referenced by your tests. You will need to install nuget test adapter or console runner to run your tests.
    </description>
    <releaseNotes>Support of netstandard, removed dependencies on aspectinjector and windows components, retrier utility, user defined settings, test suite disabling.</releaseNotes>
    <tags>automation tests steps verification matchers</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.5.2">
        <dependency id="Unicorn.Taf.Api" version="1.0.0"/>
      </group>
      <group targetFramework="net5.0">
        <dependency id="Unicorn.Taf.Api" version="1.0.0"/>
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Unicorn.Taf.Api" version="1.0.0"/>
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\icon.png" target="images\" />
    <file src="bin\Release\net452\unicorn.taf.core.dll" target="lib\net452" />
    <file src="bin\Release\net452\unicorn.taf.core.xml" target="lib\net452" />
    <file src="bin\Release\netstandard2.0\unicorn.taf.core.dll" target="lib\netstandard2.0" />
    <file src="bin\Release\netstandard2.0\unicorn.taf.core.xml" target="lib\netstandard2.0" />
    <file src="bin\Release\net5.0\unicorn.taf.core.dll" target="lib\net5.0" />
    <file src="bin\Release\net5.0\unicorn.taf.core.xml" target="lib\net5.0" />
  </files>
</package>