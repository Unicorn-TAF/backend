<?xml version="1.0"?>
<package >
  <metadata>
    <id>Unicorn.UI.Mobile</id>
    <version>1.0.0</version>
    <title>Unicorn.UI.Mobile</title>
    <authors>Vitaliy Dobriyan</authors>
    <license type="expression">MIT</license>
    <copyright>Copyright Â© 2022 Vitaliy Dobriyan</copyright>
    <projectUrl>https://bitbucket.org/dobriyanchik/unicorntaf</projectUrl>
    <repository type="git" url="https://bitbucket.org/dobriyanchik/unicorntaf"/>
    <icon>images\icon.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Unicorn.UI.Mobile is Unicorn test automation framework extension which provides ready for use implementations of interaction with Android app and web controls with extended capabilities.
      Unicorn Mobile features: Ready for use implementations of common controls, extended page object features.
      This package includes implementations of web UI interaction based on Appium.
    </description>
    <releaseNotes>First release, support of Taf.Core 3.0 and UI.Core 3.0, AndroidApplication and page objects.</releaseNotes>
    <tags>automation ui controls page-object android mobile appium</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.5.2">
        <dependency id="Appium.WebDriver" version="4.3.1"/>
        <dependency id="Selenium.Support" version="3.141.0"/>
        <dependency id="Selenium.WebDriver" version="3.141.0"/>
      </group>
      <group targetFramework="net5.0">
        <dependency id="Appium.WebDriver" version="4.3.1"/>
        <dependency id="Selenium.Support" version="3.141.0"/>
        <dependency id="Selenium.WebDriver" version="3.141.0"/>
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Appium.WebDriver" version="4.3.1"/>
        <dependency id="Selenium.Support" version="3.141.0"/>
        <dependency id="Selenium.WebDriver" version="3.141.0"/>
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\..\icon.png" target="images\" />
    <file src="bin\Release\net452\unicorn.ui.mobile.dll" target="lib\net452" />
    <file src="bin\Release\net452\unicorn.ui.mobile.xml" target="lib\net452" />
    <file src="bin\Release\netstandard2.0\unicorn.ui.mobile.dll" target="lib\netstandard2.0" />
    <file src="bin\Release\netstandard2.0\unicorn.ui.mobile.xml" target="lib\netstandard2.0" />
    <file src="bin\Release\net5.0\unicorn.ui.mobile.dll" target="lib\net5.0" />
    <file src="bin\Release\net5.0\unicorn.ui.mobile.xml" target="lib\net5.0" />
  </files>
</package>